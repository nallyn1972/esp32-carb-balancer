{
  "C_Cpp.intelliSenseEngine": "default",
  "C_Cpp.intelliSenseEngineFallback": "enabled",
  "idf.adapterTargetName": "esp32s3",
  "idf.customExtraPaths": "",
  "idf.customExtraVars": {
    "IDF_CCACHE_ENABLE": "1",
    "IDF_TARGET": "esp32s3"
  },
  "idf.flashType": "UART",
  "idf.openOcdConfigs": [
    "interface/ftdi/esp32s3_ftdi.cfg",
    "target/esp32s3.cfg"
  ],
  "idf.portWin": "COM4",
  "idf.port": "/dev/ttyUSB0",
  "files.associations": {
    "*.h": "c",
    "*.c": "c",
    "*.hpp": "cpp",
    "*.cpp": "cpp"
  },
  "C_Cpp.default.compileCommands": "${workspaceFolder}/build/compile_commands.json",
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "terminal.integrated.env.windows": {
    "PATH": "${env:PATH}"
  },
  "terminal.integrated.env.linux": {
    "PATH": "${env:PATH}"
  },
  "clangd.path": "C:\\Users\\neila\\.espressif\\tools\\esp-clang\\esp-19.1.2_20250312\\esp-clang\\bin\\clangd.exe",
  "clangd.arguments": [
    "--background-index",
    "--query-driver=C:\\Users\\neila\\.espressif\\tools\\xtensa-esp-elf\\esp-14.2.0_20241119\\xtensa-esp-elf\\bin\\xtensa-esp32s3-elf-gcc.exe",
    "--compile-commands-dir=c:\\workspace\\carb\\build"
  ]
}
